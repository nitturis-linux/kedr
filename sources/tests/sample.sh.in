#!/bin/bash

########################################################################
# This sample test just checks if the KEDR core module is present.
########################################################################
WORK_DIR=${PWD}

KEDR_MODULE="@KEDR_DIR@/@KEDR_NAME@.ko"
printf "Kedr core module: ${KEDR_MODULE}\n"

if test ! -f "${KEDR_MODULE}"; then
    printf "Kedr core module is missing: ${KEDR_MODULE}\n"
    
    # test failed
    exit 1
fi

# test passed
exit 0
