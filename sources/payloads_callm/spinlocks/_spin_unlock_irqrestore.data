[group]
	# The parameters to be output to the trace. 
	trace.param.type = spinlock_t *
	trace.param.name = lock

	trace.param.type = unsigned long
	trace.param.name = flags

	# Happens-before relationship with lock
	trace.happensBefore = yes

	# The format string to be used for trace output.
	trace.formatString = "arguments: (%p, %lu)"
#######################################################################
