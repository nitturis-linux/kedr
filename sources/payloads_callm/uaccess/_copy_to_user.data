[group]
	# The parameters to be output to the trace. 
	trace.param.type = void __user *
	trace.param.name = to

	trace.param.type = void *
	trace.param.name = from

	trace.param.type = unsigned long
	trace.param.name = n

	trace.param.type = unsigned long
	trace.param.name = returnValue
	
	# The format string to be used for trace output.
	trace.formatString = "arguments: (%p, %p, %lu), result: %lu"
