[group]
	# Name and return type of the target function
	function.name = add_to_page_cache_lru

	# Names and types of the arguments of the target function
	arg.type = struct page *
	arg.name = page
	
	arg.type = struct address_space *
	arg.name = mapping
 
	arg.type = pgoff_t
	arg.name = offset

	arg.type = gfp_t
	arg.name = gfp_mask

	# The parameters to be output to the trace. 
	trace.param.type = void *
	trace.param.name = page

	trace.param.type = void *
	trace.param.name = mapping

	trace.param.type = unsigned long
	trace.param.name = offset

	trace.param.type = unsigned int 
	trace.param.name = gfp_mask

	# Happens-before relationship with alloc_pages*() or the like
	trace.happensBefore = yes

	# The format string to be used for trace output.
	trace.formatString = "arguments: (%p, %p, %lx, %x)"

# End of the group of definitions for add_to_page_cache_lru().

