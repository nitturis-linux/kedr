[group]
	# Name and return type of the target function
	function.name = memdup_user
	returnType = void *

	# Names and types of the arguments of the target function
	arg.type = const void __user *
	arg.name = src

	arg.type = size_t
	arg.name = len

	# The parameters to be output to the trace. 
	trace.param.type = const void __user *
	trace.param.name = src

	trace.param.type = size_t
	trace.param.name = len

	trace.param.type = void *
	trace.param.name = returnValue
	
	# The format string to be used for trace output.
	trace.formatString = "arguments: (%p, %zu), result: %p"

# End of the group of definitions for memdup_user().

